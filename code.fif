"Asm.fif" include

<{
    SETCP0 IFNOTRET                                     // msg
    9 PUSHPOW2 LDSLICEX DUP HASHSU SWAP 64 LDU 16 LDU   // sign hash valid_until msg_seqno msg
    c4 PUSH CTOS                                        // sign hash valid_until msg_seqno msg c4s
    256 LDU 16 PLDU                                     // sign hash valid_until msg_seqno msg key seqno
    DUP INC 16 PUSHPOW2 MOD s2 PUSH                     // sign hash valid_until msg_seqno msg key seqno new_seqno key
    NEWC 256 STU 16 STU ENDC c4 POP                     // sign hash valid_until msg_seqno msg key seqno
    s4 s3 s0 XCHG3 s4 s6 XCHG                           // msg hash sign key valid_until msg_seqno seqno
    EQUAL 33 THROWIFNOT                                 // msg hash sign key valid_until
    NOW GEQ 34 THROWIFNOT                               // msg hash sign key
    CHKSIGNU 35 THROWIFNOT                              // msg
    ACCEPT COMMIT                                       // msg
    PLDREF c5 POP
}>c
